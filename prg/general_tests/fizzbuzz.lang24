buzzing(n:int):void = {
	if(n % 3 == 0 and
	   n % 5 == 0) then {
		putcstr("FizzBuzz\n", 10);
		return none;
	}

	if(n % 3 == 0) then {
		putcstr("Fizz\n", 6);
		return none;
	}

	if(n % 5 == 0) then {
		putcstr("Buzz\n", 6);
		return none;
	}

	putint(n);
	putchar('\n');
	return none;
}

