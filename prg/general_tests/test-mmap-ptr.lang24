putint(i:int):void
putchar(c:char):void
exit(i:int):void
mmap(len:int, size:int):^void
seed:int

main():int=
	{
		i = <^int>mmap(10, sizeof(int));
		cmps = 0;
		while (cmps < 10) {
			i[cmps] = cmps;
			cmps = cmps + 1;
		}
		
		j = <^int>mmap(10, sizeof(int));
		cmps = 0;
		while (cmps < 10) {
			j[cmps] = cmps + 10;
			cmps = cmps + 1;
		}

		cmps = 0;
		while (cmps < 10){
			putint(i[cmps]);
			putchar('\n');
			cmps = cmps + 1;
		}
		putchar('\n');

		cmps = 0;
		while (cmps < 10) {
			putint(j[cmps]);
			putchar('\n');
			cmps = cmps + 1;
		}

		putchar('\n');
		exit(0);
	}
	{i:^int cmps:int j:^int}
